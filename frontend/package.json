{
  "name": "sweet-shop-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:full": "concurrently -n \"REACT,FASTAPI\" -c \"green,blue\" \"npm run dev\" \"cd ../backend && uvicorn app.main:app --reload\"",
